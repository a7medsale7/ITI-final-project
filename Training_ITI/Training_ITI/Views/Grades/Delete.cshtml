@model Training_ITI.Models.Grade
@{
    ViewData["Title"] = "Delete Grade";
}
<h3>Delete Grade</h3>
<div class="alert alert-warning">
    Are you sure you want to delete this grade for <strong>@Model.Trainee?.Name</strong>?
</div>
<form method="post">
    <button class="btn btn-danger" type="submit">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
